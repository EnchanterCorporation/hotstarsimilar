<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.enchcrop.hotstarsimilar">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Main3Activity"
            android:label="@string/title_activity_main3"
            android:parentActivityName=".MainActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.enchcrop.hotstarsimilar.MainActivity" />
        </activity>
        <activity
            android:name=".youplay"
            android:label="@string/title_activity_youplay"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".list2min"
            android:label="@string/title_activity_list2min"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".youplay1"
            android:label="@string/title_activity_youplay1"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".youplay3"
            android:label="@string/title_activity_youplay3"
            android:theme="@style/AppTheme.NoActionBar"></activity>
    </application>

</manifest>